{
  "name": "tapknight",
  "version": "0.0.1",
  "description": "Some game or something?",
  "main": "_build/top/app.js",
  "engines": {
    "node": "v6.1.0"
  },
  "scripts": {
    "start": "node _build/src/server.js",
    "build": "rebel && webpack --config webpack.dev.js _build/src/client.js"
